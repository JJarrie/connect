{
    "name": "symfonycorp/connect",
    "description": "SymfonyConnect SDK",
    "keywords": ["SymfonyConnect", "SDK"],
    "homepage": "https://github.com/symfonycorp/connect",
    "type": "library",
    "license": "MIT",
    "authors": [],
    "require": {
        "kriswallsmith/buzz": ">=0.8,<0.17",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "symfony/form": "^2.1|^3.0|^4.0",
        "symfony/security": "^2.3|^3.0|^4.0",
        "symfony/phpunit-bridge": "^4.0"
    },
    "suggest": {
        "symfony/security": "To use in a Symfony environment"
    },
    "autoload": {
        "psr-0": {
            "SymfonyCorp": ["src/"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-migrate-to-symfonyconnect": "5.x-dev"
        }
    }
}

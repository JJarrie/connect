{
    "name": "sensiolabs/connect",
    "description": "SensioLabs Connect SDK",
    "keywords": ["SensioLabs Connect", "SDK"],
    "homepage": "https://github.com/sensiolabs/connect",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Weistroff",
            "email": "marc.weistroff@sensiolabs.com",
            "homepage": "http://sensiolabs.com"
        }
    ],
    "require": {
        "kriswallsmith/buzz": ">=0.8,<0.17",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "symfony/form": "^2.1|^3.0|^4.0",
        "symfony/security": "^2.3|^3.0|^4.0",
        "symfony/phpunit-bridge": "^4.2"
    },
    "suggest": {
        "symfony/security": "To use in a Symfony environment"
    },
    "autoload": {
        "psr-0": {
            "SensioLabs": ["src/"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
